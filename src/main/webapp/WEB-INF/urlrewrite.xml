<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
        <note>note</note>
        <from>/</from>
        <to type="forward">index.do</to>
    </rule>
    <rule>
        <note>note</note>
        <from>/(.*)</from>
        <to type="forward">$1.do</to>
    </rule>
</urlrewrite>

